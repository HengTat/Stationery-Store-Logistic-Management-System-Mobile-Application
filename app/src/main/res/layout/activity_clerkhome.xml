<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:layout_width="match_parent"
        android:layout_height="75dp"
        android:background="@drawable/homepageheader"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/retrievalicon"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.2"
        android:layout_width="187dp"
        android:layout_height="187dp"
        android:src="@drawable/ic_action_name"
        tools:layout_editor_absoluteX="112dp"
        tools:layout_editor_absoluteY="73dp"/>

    <Button
        android:id="@+id/retrieval"
        android:layout_width="180dp"
        android:layout_height="49dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:text="Retrieval Form"
        android:background="@drawable/button_functions"
        android:textColor="@android:color/white"
        tools:layout_editor_absoluteX="115dp"
        tools:layout_editor_absoluteY="283dp" />

    <Button
        android:id="@+id/disbursement"
        android:layout_width="180dp"
        android:layout_height="49dp"
        android:text="Disbursement Form"
        android:background="@drawable/button_functions"
        android:textColor="@android:color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.536"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.894" />

    <View
        android:id="@+id/divider"
        android:layout_width="409dp"
        android:layout_height="1dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintVertical_bias="0.55"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="371dp" />

    <ImageView
        android:id="@+id/disbursementicon"
        android:layout_width="163dp"
        android:layout_height="168dp"
        app:srcCompat="@drawable/ic_form"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintVertical_bias="0.76"
        tools:layout_editor_absoluteX="123dp"
        tools:layout_editor_absoluteY="408dp" />

    <Button
        android:id="@+id/logout"
        android:background="@drawable/button_view_details"
        android:textColor="@android:color/white"
        tools:layout_editor_absoluteY="16dp"
        android:drawableStart="@drawable/ic_baseline_exit_to_app_24"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintVertical_bias="0.02"
        android:text=" Logout"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/usericon"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="15dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.04"
        app:layout_constraintVertical_bias="0.02"
        android:layout_width="49dp"
        android:layout_height="49dp"
        app:srcCompat="@drawable/ic_user" />

    <TextView
        android:id="@+id/welcome"
        tools:layout_editor_absoluteX="75dp"
        tools:layout_editor_absoluteY="29dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@id/usericon"
        app:layout_constraintVertical_bias="0.04"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Welcome "
        android:textColor="@android:color/white"/>/>

    <TextView
        android:id="@+id/name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@id/welcome"
        app:layout_constraintVertical_bias="0.04"
        tools:layout_editor_absoluteX="75dp"
        tools:layout_editor_absoluteY="29dp"
        android:textColor="@android:color/white"/>

</androidx.constraintlayout.widget.ConstraintLayout>